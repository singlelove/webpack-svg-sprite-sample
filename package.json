{
  "name": "webpack-svg-sprite",
  "description": "Webpack SVG sprite sample project",
  "author": "vagusX <vagusxl@gmail.com>",
  "scripts": {
    "dev:vue": "node ./node_modules/.bin/webpack-dev-server --port 8001 --config webpack.vue.conf.js --inline --hot",
    "build:vue": "NODE_ENV=production webpack --config webpack.vue.conf.js --progress --hide-modules",
    "dev:react": "webpack-dev-server --progress --port 8002 --config webpack.react.conf.js --inline --hot",
    "build:react": "webpack --port 8002 --config webpack.react.conf.js --progress --hide-modules --hot"
  },
  "dependencies": {
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "vue": "^1.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-loader": "^6.0.0",
    "babel-plugin-transform-runtime": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.0.0",
    "css-loader": "^0.23.0",
    "file-loader": "^0.8.4",
    "json-loader": "^0.5.4",
    "style-loader": "^0.13.1",
    "svg-sprite-loader": "^3.0.5",
    "svgo-loader": "^1.1.0",
    "url-loader": "^0.5.7",
    "vue-hot-reload-api": "^1.2.0",
    "vue-html-loader": "^1.0.0",
    "vue-loader": "^8.2.1",
    "vue-style-loader": "^1.0.0",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.0"
  }
}
